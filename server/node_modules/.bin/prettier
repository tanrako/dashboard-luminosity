#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/.pnpm/prettier@2.8.8/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/.pnpm/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules:/Users/tanjonarakotoarisoa/Tan/node_modules:/Users/tanjonarakotoarisoa/node_modules:/Users/node_modules:/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/prettier/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/.pnpm/prettier@2.8.8/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/.pnpm/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules:/Users/tanjonarakotoarisoa/Tan/node_modules:/Users/tanjonarakotoarisoa/node_modules:/Users/node_modules:/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/server/node_modules/prettier/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../prettier/bin-prettier.js" "$@"
fi
