#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/typescript@5.1.3/node_modules/typescript/bin/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/typescript@5.1.3/node_modules/typescript/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/typescript@5.1.3/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules:/Users/tanjonarakotoarisoa/Tan/node_modules:/Users/tanjonarakotoarisoa/node_modules:/Users/node_modules:/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/ts-node@10.9.1_803289a29b5e0a2f51bd79e13d6672bf/node_modules/ts-node/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/ts-node@10.9.1_803289a29b5e0a2f51bd79e13d6672bf/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/typescript@5.1.3/node_modules/typescript/bin/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/typescript@5.1.3/node_modules/typescript/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/typescript@5.1.3/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules:/Users/tanjonarakotoarisoa/Tan/node_modules:/Users/tanjonarakotoarisoa/node_modules:/Users/node_modules:/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/ts-node@10.9.1_803289a29b5e0a2f51bd79e13d6672bf/node_modules/ts-node/node_modules:/Users/tanjonarakotoarisoa/Tan/solar-farm/node_modules/.pnpm/ts-node@10.9.1_803289a29b5e0a2f51bd79e13d6672bf/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.1.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.1.3/node_modules/typescript/bin/tsserver" "$@"
fi
